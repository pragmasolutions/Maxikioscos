@model MaxiKioscos.Web.Models.StockModel
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title" id="myModalLabel">Editar Stock</h4>
</div>
@using (Html.BeginForm("Editar", "Stocks"))
{
    <div class="modal-body">
        @{Html.RenderPartial("_CamposStockEditar"); }
    </div>
    <div class="modal-footer">
        <button type="submit" class="btn btn-default"><span class="glyphicon glyphicon-floppy-disk"></span>&nbsp;&nbsp;Editar</button>
        <button type="button" class="btn btn-default" data-dismiss="modal"><span class="glyphicon glyphicon-remove"></span>Cancelar</button>
    </div>
}

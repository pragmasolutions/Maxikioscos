@using MaxiKioscos.Web.Comun.Extensiones
@model List<MaxiKioscos.Entidades.PermisoRol>
@{
    ViewBag.Title = ViewBag.ListAction + " de rol: " + ViewBag.RoleName;
}

@Html.Partial("PermisosListado")
@Html.Modal("RolesModal")
@if (!IsAjax)
{
    <text>
    @section scripts
    {
        @Scripts.Render("~/Scripts/app/maxikiosco-roles-reportes-permisos.js")
    }
    </text>
}
else
{
    @Scripts.Render("~/Scripts/app/maxikiosco-roles-reportes-permisos.js")
}
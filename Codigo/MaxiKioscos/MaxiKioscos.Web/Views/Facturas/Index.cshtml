@using MaxiKioscos.Web.Comun.Extensiones
@model MaxiKioscos.Web.Models.FacturasListadoModel
@{
    ViewBag.Title = "Facturas";
}
@Html.Partial("Listado")
@Html.Modal("FacturasModal")
@if (!IsAjax)
{
    <text>
    @section scripts
    {
        @Scripts.Render("~/Scripts/app/maxikiosco-facturas.js")
    }
    </text>
}
else
{
    @Scripts.Render("~/Scripts/app/maxikiosco-facturas.js")

}

<script>
    $(function () {
        $('[data-toggle="tooltip"]').tooltip();
    })
</script>
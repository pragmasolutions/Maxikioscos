@model MaxiKioscos.Entidades.Factura
@Html.HiddenFor(model => model.FacturaId)
@Html.HiddenFor(model => model.Identifier)
@Html.HiddenFor(model => model.Desincronizado)
@Html.HiddenFor(model => model.Eliminado)
@Html.HiddenFor(model => model.FechaCreacion)
@Html.HiddenFor(model => model.AutoNumero)
@Html.HiddenFor(model => model.CierreCajaId)

<div class="form-group">
    @Html.LabelFor(model => model.Fecha)
    @Html.EditorFor(model => model.Fecha)
    @Html.ValidationMessageFor(model => model.Fecha)
</div>
<div class="form-group">
    @Html.LabelFor(model => model.FacturaNro)
    @Html.EditorFor(model => model.FacturaNro)
    @Html.ValidationMessageFor(model => model.FacturaNro)
</div>
<div class="form-group">
    @Html.LabelFor(model => model.ProveedorId)<br />
    @Html.DropDownListFor(model => model.ProveedorId, ViewBag.Proveedores as SelectList, new { @data_searchable = "true", @data_placeholder = "Seleccione Proveedor", @class = "form-dropdown"})
    @Html.ValidationMessageFor(model => model.ProveedorId)
</div>
<div class="form-group">
    @Html.LabelFor(model => model.ImporteTotal)
    @Html.EditorFor(model => model.ImporteTotal)
    @Html.ValidationMessageFor(model => model.ImporteTotal)
</div>
<div class="form-group">
    @Html.LabelFor(model => model.MaxiKioscoId)<br />
    @Html.DropDownListFor(model => model.MaxiKioscoId, ViewBag.MaxiKioscos as SelectList, new { @data_searchable = "true", @data_placeholder = "Seleccione Maxikiosco", @class = "form-dropdown"})
    @Html.ValidationMessageFor(model => model.MaxiKioscoId)
</div>


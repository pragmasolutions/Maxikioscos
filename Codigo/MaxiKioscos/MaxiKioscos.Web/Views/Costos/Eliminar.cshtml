@using MaxiKioscos.Web.Comun.Extensiones
@model MaxiKioscos.Entidades.Costo

@Html.PopupTitulo("Eliminar Costo")

@using (Html.BeginForm("Eliminar", "Costos"))
{
    <div class="modal-body">
        <div class="row">
            <div>
                <div class="alert alert-warning">
                    Esta seguro que desea eliminar el costo?
                </div>
            </div>
            @{Html.RenderPartial("_CamposCostoDetalle"); }
        </div>
    </div>
    <div class="modal-footer">
        <button type="submit" class="btn btn-default"><span class="glyphicon glyphicon-trash"></span>&nbsp;&nbsp;Eliminar</button>
        <button type="button" class="btn btn-default" data-dismiss="modal"><span class="glyphicon glyphicon-remove"></span>Cancelar</button>
    </div>
}

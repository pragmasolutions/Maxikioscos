@using MaxiKioscos.Web.Comun.Extensiones
@using PagedList
@using PagedList.Mvc
@model MaxiKioscos.Web.Models.CostosListadoModel
@Html.PaginaTitulo("Gastos","fa-dollar")
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <a class="btn btn-default btn-costo-crear" href="@Url.Action("Crear")"><span class="glyphicon glyphicon-plus"></span>&nbsp;&nbsp;Crear Gasto</a>
            </div>
            <div class="panel-body">
                <div class="spacer"></div>
                @Html.Partial("_Filtros", Model.Filtros)
                <div class="spacer"></div>
                <div class="spacer"></div>
                @Html.Partial("_Listado", Model)
                <div class="spacer"></div>
            </div>
        </div>
    </div>
</div>
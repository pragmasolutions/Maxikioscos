@using MaxiKioscos.Web.Comun.Extensiones
@model MaxiKioscos.Entidades.Costo

@Html.PopupTitulo("Crear Gasto")
@using (Html.BeginForm("Crear", "Costos"))
{
    <div class="modal-body">
        <div class="row">
            @{Html.RenderPartial("_CamposCostoEditar"); }
        </div>
    </div>
    <div class="modal-footer">
        <button type="submit" class="btn btn-default"><span class="glyphicon glyphicon-floppy-disk"></span>&nbsp;&nbsp;Crear</button>
        <button type="button" class="btn btn-default" data-dismiss="modal"><span class="glyphicon glyphicon-remove"></span>Cancelar</button>
    </div>
}



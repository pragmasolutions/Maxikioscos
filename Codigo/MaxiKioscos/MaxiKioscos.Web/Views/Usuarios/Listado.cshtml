@using MaxiKioscos.Web.Comun.Extensiones
@model MaxiKioscos.Web.Models.UsuariosListadoModel

@Html.PaginaTitulo("Usuarios","fa-user")
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <a class="btn btn-default btn-usuario-crear" href="@Url.Action("Crear")"><span class="glyphicon glyphicon-plus"></span>&nbsp;&nbsp;Crear Usuario</a>
            </div>
            <div class="panel-body">
                <div class="spacer"></div>
                @Html.Partial("_Filtros",Model.Filtros)
                <div class="spacer"></div>
                <div class="spacer"></div>
                @Html.Partial("_Listado", Model)
                <div class="spacer"></div>
            </div>
        </div>
    </div>
</div>
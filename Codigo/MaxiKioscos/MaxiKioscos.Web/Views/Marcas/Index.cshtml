@using MaxiKioscos.Web.Comun.Extensiones
@model MaxiKioscos.Web.Models.MarcasListadoModel
@{
    ViewBag.Title = "Marcas";
}

@Html.Partial("Listado")
@Html.Modal("MarcasModal")
@if (!IsAjax)
{

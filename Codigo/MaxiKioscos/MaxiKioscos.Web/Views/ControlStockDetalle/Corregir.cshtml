@using MaxiKioscos.Web.Comun.Extensiones
@model MaxiKioscos.Entidades.ControlStockDetalle
@{
    ViewBag.Title = "Corregir Stock";
}

@Html.PopupTitulo("Corregir Stock")
@using (Html.BeginForm("Corregir", "ControlStockDetalle"))
{
    <div class="modal-body">
        <div class="row">
            <div class="col-lg-12">
                @{ Html.RenderPartial("_CamposCorregir"); }
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <button type="submit" class="btn btn-default"><span class="glyphicon glyphicon-floppy-disk"></span>&nbsp;&nbsp;Corregir</button>
        <button type="button" class="btn btn-default" data-dismiss="modal"><span class="glyphicon glyphicon-remove"></span>Cancelar</button>
    </div>  
}
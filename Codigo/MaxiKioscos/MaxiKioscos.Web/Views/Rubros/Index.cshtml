@using MaxiKioscos.Web.Comun.Extensiones
@model MaxiKioscos.Web.Models.RubrosListadoModel
@{
    ViewBag.Title = "Rubros";
}

@Html.Partial("Listado")
@Html.Modal("RubrosModal")
@if (!IsAjax)
{
    <text>
    @section scripts
    {
        @Scripts.Render("~/Scripts/app/maxikiosco-rubros.js")
    }
    </text>
}
else
{
    @Scripts.Render("~/Scripts/app/maxikiosco-rubros.js")
}
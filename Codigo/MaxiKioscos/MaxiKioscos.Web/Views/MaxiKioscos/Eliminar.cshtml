@using MaxiKioscos.Web.Comun.Extensiones
@model MaxiKioscos.Entidades.MaxiKiosco

@Html.PopupTitulo("Eliminar MaxiKiosco")
@using (Html.BeginForm("Eliminar", "MaxiKioscos"))
{
    <div class="modal-body">
        <div class="row">
            <div>
                <div class="alert alert-warning">
                    ¿Esta seguro que desea eliminar el siguiente MaxiKiosco?
                </div>
            </div>
        </div>
        @{Html.RenderPartial("_CamposMaxiKioscoDetalle"); }
    </div>
    <div class="modal-footer">
        <button type="submit" class="btn btn-default"><span class="glyphicon glyphicon-trash"></span>&nbsp;&nbsp;Eliminar</button>
        <button type="button" class="btn btn-default" data-dismiss="modal"><span class="glyphicon glyphicon-remove"></span>Cancelar</button>
    </div>
}
